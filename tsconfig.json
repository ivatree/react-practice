{
    "compilerOptions": {
      "typeRoots": ["./node_modules/@types", "./types"],
      "types": ["react", "react-dom", "react-redux", "node"],
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": ["dom", "esnext"],
      "jsx": "react",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "src"
    },
    "exclude": ["node_modules"],
  }